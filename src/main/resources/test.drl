package openehr.test;

import java.util.*;
import org.drools.core.factmodel.traits.Alias

global java.util.List list;

declare FactA
    fieldB: FactB
end

declare FactB
    fieldA: FactA
end

rule R1 when
   $fieldA : FactA( $fieldB : fieldB )
   FactB( this == $fieldB, fieldA == $fieldA )
then
end